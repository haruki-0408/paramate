{
  "name": "paramate",
  "version": "1.0.4",
  "description": "AWS Parameter StoreにCSVからパラメータを投入するCLIツール",
  "main": "dist/index.js",
  "bin": {
    "prm": "dist/cli/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/cli.js",
    "dev": "ts-node src/cli/cli.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run type-check && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "aws",
    "parameter-store",
    "csv",
    "put",
    "rollback",
    "cli",
    "typescript"
  ],
  "author": "a3a",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haruki-0408/paramate.git"
  },
  "bugs": {
    "url": "https://github.com/haruki-0408/paramate/issues"
  },
  "homepage": "https://github.com/haruki-0408/paramate#readme",
  "dependencies": {
    "@aws-sdk/client-kms": "^3.700.0",
    "@aws-sdk/client-ssm": "^3.700.0",
    "@aws-sdk/client-sts": "^3.700.0",
    "@aws-sdk/config-resolver": "^3.370.0",
    "@aws-sdk/credential-providers": "^3.844.0",
    "@aws-sdk/node-config-provider": "^3.370.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "5.9.2"
  }
}
